{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
      "quotemark": false,
      "prefer-const": false,
      "one-line": false,
      "align": false,
      "no-string-literal": false,
      "no-trailing-whitespace": false
    }
}
